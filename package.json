{
  "name": "@boldcommerce/checkout-experience-templates",
  "version": "1.0.1-build.190",
  "description": "Code base to generate the Javascript template to load Checkout using Bold Checkout Storefront APIs",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bold-commerce/checkout-experience-templates.git"
  },
  "scripts": {
    "preconfig": "node createRequiredConfig.js",
    "prerelease": "npm --no-git-tag-version --preid=build version prerelease && node libVersion.js > src/constants/version.ts",
    "prebuild": "yarn preconfig && yarn prerelease",
    "test": "jest",
    "cover-test": "jest --coverage --colors",
    "build": "webpack --bail --mode production",
    "build-dev": "yarn prebuild && webpack --bail --mode development",
    "webpack": "yarn prebuild && webpack",
    "start": "yarn prebuild && NODE_ENV=development webpack --watch",
    "serve": "yarn prebuild && NODE_ENV=development webpack serve",
    "lint": "eslint ./src --fix",
    "lint-ci": "node_modules/.bin/eslint src",
    "test-ci": "yarn cover-test --ci --bail --verbose --reporters=default --reporters=jest-junit --forceExit --watchAll=false --detectOpenHandles"
  },
  "keywords": [],
  "devDependencies": {
